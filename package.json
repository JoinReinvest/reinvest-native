{
  "name": "reinvest-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "yarn eslint ./src",
    "lint:fix": "yarn lint --fix",
    "check-types": "tsc",
    "start": "react-native start",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock && yarn",
    "nuke": "bash scripts/react-native-nuke.sh"
  },
  "dependencies": {
    "@aws-amplify/auth": "5.3.6",
    "@aws-amplify/core": "5.3.0",
    "@gorhom/bottom-sheet": "^4.4.6",
    "@hookform/resolvers": "3.1.0",
    "@react-native-async-storage/async-storage": "1.18.1",
    "@react-native-community/netinfo": "9.3.10",
    "@react-native-firebase/app": "^18.0.0",
    "@react-native-firebase/messaging": "^18.0.0",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@sentry/react-native": "^5.4.2",
    "@shopify/flash-list": "^1.4.3",
    "@tanstack/react-query": "4.29.7",
    "amazon-cognito-identity-js": "6.2.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "graphql": "16.6.0",
    "graphql-request": "6.0.0",
    "jotai": "^2.1.0",
    "lefthook": "1.3.13",
    "lodash.isequal": "^4.5.0",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-native": "0.71.10",
    "react-native-avoid-softinput": "3.1.5",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.6.0",
    "react-native-document-picker": "9.0.1",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "^2.10.0",
    "react-native-image-picker": "5.3.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-maps": "^1.7.0",
    "react-native-mask-input": "1.2.3",
    "react-native-reanimated": "3.1.0",
    "react-native-responsive-linechart": "^5.7.1",
    "react-native-responsive-screen": "1.4.2",
    "react-native-safe-area-context": "4.5.2",
    "react-native-screens": "3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "13.9.0",
    "react-native-video": "5.2.1",
    "react-native-webview": "12.1.0",
    "reinvest-app-common": "git+ssh://git@github.com:JoinReinvest/reinvest-fe-common.git"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "^7.21.5",
    "@hookooekoo/configs-tsconfig": "0.0.17",
    "@hookooekoo/eslint-config": "0.2.2",
    "@tanstack/eslint-plugin-query": "4.29.4",
    "@types/lodash.isequal": "^4.5.6",
    "@types/react": "18.2.6",
    "@types/react-native-video": "5.0.14",
    "babel-plugin-module-resolver": "5.0.0",
    "metro-react-native-babel-preset": "0.76.4",
    "typescript": "5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "@hookooekoo/eslint-config/react-native",
      "plugin:@tanstack/eslint-plugin-query/recommended"
    ],
    "plugins": [
      "@tanstack/query"
    ],
    "rules": {
      "@tanstack/query/exhaustive-deps": 0,
      "@tanstack/query/prefer-query-object-syntax": "error",
      "import/extensions": "off",
      "security/detect-object-injection": "off"
    }
  },
  "prettier": "@hookooekoo/eslint-config/prettier.json"
}
