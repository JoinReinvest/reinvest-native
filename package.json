{
  "name": "reinvest-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "yarn eslint ./src",
    "lint:fix": "yarn lint --fix",
    "check-types": "tsc --pretty",
    "start": "react-native start",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock && yarn",
    "nuke": "bash scripts/react-native-nuke.sh"
  },
  "dependencies": {
    "@aws-amplify/auth": "5.2.0",
    "@aws-amplify/core": "5.1.5",
    "@hookform/resolvers": "3.0.0",
    "@react-native-async-storage/async-storage": "1.18.0",
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@tanstack/react-query": "4.28.0",
    "amazon-cognito-identity-js": "6.2.0",
    "lefthook": "1.3.8",
    "dayjs": "^1.11.7",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "react": "18.2.0",
    "react-hook-form": "7.43.8",
    "react-native": "0.71.4",
    "react-native-avoid-softinput": "^3.1.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-image-picker": "5.3.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mask-input": "1.2.3",
    "react-native-reanimated": "3.0.2",
    "react-native-responsive-screen": "1.4.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.8.0",
    "react-native-video": "5.2.1",
    "react-native-webview": "^11.26.1",
    "reinvest-app-common": "git+ssh://git@github.com:JoinReinvest/reinvest-fe-common.git"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "^7.21.0",
    "@hookooekoo/configs-tsconfig": "0.0.15",
    "@hookooekoo/eslint-config": "0.2.1",
    "@tanstack/eslint-plugin-query": "4.27.0",
    "@types/react": "18.0.30",
    "@types/react-native-video": "5.0.14",
    "babel-plugin-module-resolver": "5.0.0",
    "metro-react-native-babel-preset": "0.76.0",
    "react-native-dotenv": "3.4.8",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "@hookooekoo/eslint-config/react-native",
      "plugin:@tanstack/eslint-plugin-query/recommended"
    ],
    "plugins": [
      "@tanstack/query"
    ],
    "rules": {
      "@tanstack/query/exhaustive-deps": 0,
      "@tanstack/query/prefer-query-object-syntax": "error"
    }
  },
  "prettier": "@hookooekoo/eslint-config/prettier.json"
}
